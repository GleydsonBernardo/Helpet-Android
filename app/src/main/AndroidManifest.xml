<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.novaroma.helpet_android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presentation.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.Home"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".presentation.Register"
            android:label="@string/title_activity_register"
            android:theme="@style/AppTheme.NoActionBar"></activity>

        <activity
            android:name=".presentation.RegisterAddress"
            android:label="@string/title_activity_register_address"
            android:theme="@style/AppTheme.NoActionBar"></activity>

    </application>

</manifest>